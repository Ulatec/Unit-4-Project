
{{#partial "content"}}
        <section>
            <div class="container">
                <div class="entry-list">
                    {{#each posts}}
                        <article>
                        <h2><a href="/detail/{{ slug }}">{{ title }}</a></h2>
                        <time datetime="{{ creationDate }}"> {{ creationDate }}</time>
                        </article>
                    {{/each}}
                </div>
            </div>
        </section>
{{/partial}}
{{> base.hbs}}